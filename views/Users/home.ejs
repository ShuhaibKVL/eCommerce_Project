    <script>

    function handleSearchFormSubmit(event) {
        console.log("The search input function invoked ");
        const searchInput = document.getElementById('search_input').value;

        fetch('/searchFromNavbar', {
            method:'POST',
            headers:{
                'Content-Type': 'application/json'
            },
            body:JSON.stringify({query:searchInput})
        })
        .then(Response => Response.json())
        .then(data => {
            console.log(data);
        
            // Redirect to the product page with the search results
        
            window.location.href = '/loadSearchProduct?query='+ encodeURIComponent(data.searchInput)
        
        })
        .catch(error => {
            console.log("Error on handleSearchFormSubmit",error);
        })

    }

    const searchForm = document.getElementById('searchForm');
    searchForm.addEventListener('submit', handleSearchFormSubmit);

    </script>

