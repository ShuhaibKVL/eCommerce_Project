    <section class="confirmation_part padding_top">
        <% if(typeof OrderDetails !== 'undefined' ) { %>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="confirmation_tittle">
                            <span style="color: green !important;"></span>
                        </div>
                    </div>
                    <div class="col-lg-6 col-lx-4">
                        <div class="single_confirmation_details" style="background-color: rgb(255, 255, 255); border: 1px dotted rgb(255, 225, 188);">

                            <h4>order Details :</h4>
                            <ul>
                                <li>
                                    <p>order id</p><span>: <%= OrderDetails.OderId %></span>
                                </li>
                                <li>
                                    <p>date</p><span>: <%= OrderDetails..createdAt.toLocaleDateString() %></span>
                                </li>
                                <li>
                                    <p>total</p><span>: $<%= OrderDetails.totalAmount %>.00</span>
                                </li>
                                <li>
                                    <p>Payment Method</p><span>: <%= OrderDetails.PaymentMethod  %></span>
                                </li>
                                <li>
                                    <p>Payment Id</p><span>: <%= OrderDetails.PaymentId  %></span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    
                    <div class="col-lg-6 col-lx-4">
                        <div class="single_confirmation_details"  style="background-color: rgb(255, 255, 255); border: 1px dotted rgb(255, 225, 188);">
                            <h4>shipping Address</h4>
                            
                            <ul>
                                <li>
                                    <p>Name</p><span>: <%= OrderDetails.Deliver_Address.FirstName %> <%= OrderDetails.Deliver_Address.LastName %> </span>
                                </li>
                                <li>
                                    <p>Ph </p><span>: <%= OrderDetails.Deliver_Address.PhoneNumber %> </span>
                                </li>
                                <li>
                                    <p>Email</p><span>: <%= OrderDetails.Deliver_Address.Email %> </span>
                                </li>
                                <li>
                                    <p>Address</p><span>: <%= OrderDetails.Deliver_Address.add1 %></span>
                                </li>
                                <li>
                                    <p></p><span> <%= OrderDetails.Deliver_Address.add2 %></span>
                                </li>
                                <li>
                                    <p>Town</p><span>: <%= OrderDetails.Deliver_Address.Town %></span>
                                </li>
                                <li>
                                    <p>PinCode</p><span>: <%= OrderDetails.Deliver_Address.PinCode %></span>
                                </li>
                                <li>
                                    <p>District</p><span>: <%= OrderDetails.Deliver_Address.District %> </span>
                                </li>
                                <li>
                                    <p>State</p><span>: <%= OrderDetails.Deliver_Address.State %> </span>
                                </li>
                                <li>
                                    <p>Country</p><span>: <%= OrderDetails.Deliver_Address.Country %> </span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12 mt-4" style="border: 1px dotted rgb(255, 225, 188); ">
                        <div class="order_details_iner"  style="background-color: rgb(255, 255, 255);">
                            <div class="container d-flex justify-content-center"><h3>Products</h3></div>
                            
                            
                            <div class="accordion-body">
                            <!-- Loop through products within the order and display detailed information -->
                            <% OrderDetails.OrderItems.forEach(item => { %>
                                
                                    <div class="container-fluid"
                                        style="display: flex; flex-wrap: wrap; justify-content: space-between; align-items: center;">
                                        <a href="/ProductDetails?id=<%= item.ProductId._id %>" >
                                        <div class="image_container">
                                            <img src="/assets/uploads/<%= item.ProductId.Image[0] %>"
                                                class="product-image"
                                                alt="...Loading">
                                        </div>
                                        </a>
                                        <div
                                            style=" width: 80%; display: block; border: 1px solid rgb(255, 211, 211); border-radius: 30px;">
                                            <div id="OderDetails"
                                                style="margin: 1vw; padding: 0px !important;">
                                                <p><span>
                                                        <%= item.ProductId.Name
                                                            %>
                                                    </span></p>
                                                <p>
                                                    <%= item.ProductId.Description
                                                        %> <span>Size
                                                            :</span>
                                                        <%= item.ProductId.Size
                                                            %>
                                                </p>
                                                <p><span>Quantity :</span>
                                                    <%= item.quantity
                                                        %>
                                                </p>
                                                <p><span>Price :</span>$<%=
                                                        item.Price
                                                        %>
                                                </p>
                                                <p><span
                                                        style="color: green;">Oder
                                                        Satatus :</span>
                                                    <%= item.OrderStatus
                                                        %>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <!-- Add other product details as -->
                                    <% }) %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <% } %>
        <div class="container" style="width: 100%; display: flex; justify-content: end; flex-wrap: wrap; margin-top: 1vw;">
            <a class="btn_1 checkout_btn_1" style=" background-color: rgb(255, 47, 134);" href="/LoadOrderManagment">See Orders</a></div>
       
    </section>
