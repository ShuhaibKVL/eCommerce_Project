    <!--================ whishList_part Area =================-->
    
    <div id="reloadDiv">

        <section class="cart_area padding_top">
            <div class="container-fluid" style=" width:400px; height: 50px;">
                <p id="out-stock-msg" style="color: red;"></p>
            </div>
            <div class="container">
                <div class="cart_inner">
                    <% if (Product.length > 0) { %>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th scope="col">Product</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% Product.forEach(item => { %>
                                        <tr>
                                            <td>
                                                <a style="text-decoration: none;" href="/ProductDetails?id=<%= item.ProductId._id %>"  >
                                                    <div class="media">
                                                    <div class="d-flex ">
                                                        <img src="/assets/uploads/<%= item.ProductId.Image[0] %>" alt=""
                                                        width="60" height="80" />
                                                    </div>
                                                    <div class="media-body">
                                                        <p><%= item.ProductId.Name %></p>
                                                        <p><%= item.ProductId.Description %></p>
                                                        <h5>$<%= item.ProductId.Price %></h5>
                                                    </div>
                                                    </div>
                                                </a>
                                            </td>
                                            <td>
                                                <form action="remove-from-whishlist" method="post">
                                                    <input type="hidden" name="productId" value="<%= item.ProductId._id %>">
                                                    <button class="btn_3" type="submit">Remove</button>
                                                    
                                                </form>
                                                
                                                
                                            </td>
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
    
                            <div class="checkout_btn_inner float-right">
                                <br>
                                <a class="btn_1" href="/">Continue Shopping</a>
                                
                            </div>
                        </div>
                    <% } else { %>
                        <h2>Your wishlist is empty.</h2>
                    <% } %>
                </div>
            </div>
        </section>
    
    </div>
    




