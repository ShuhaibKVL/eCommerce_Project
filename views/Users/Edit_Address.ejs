                <input type="text" class="form-control" value="<%= Address.PhoneNumber %>" id="number" name="number" />
                <!-- // <span class="placeholder" data-placeholder="Phone number"></span> -->

                <input type="hidden"  value="<%= Address._id %>"  id="number" name="_id" />

                <input type="hidden"  value="<%= Address.UserId %>"  id="number" name="UserId" />

              </div>

              <label for="">Email</label>
              <div class="col-md-6 form-group p_star">
                <input type="text" class="form-control" value="<%= Address.Email %>" id="email" name="email" />
               
              </div>

              <div class="col-md-12 form-group p_star"  style="display: flex;">
                <select class="country_select" name="Country">
                  <option value="India">India</option>
                  <option value="USA">USA</option>
                  <option value="Caneda">Caneda</option>
                </select>

                <select class="country_select" name="State">
                    <option value="Kerala">Kerala</option>
                    <option value="Tamil Nadu">Tamil Nadu</option>
                    <option value="Karnadaka">Karnadaka</option>
                  </select>

                  <select class="country_select" name="District">
                    <option value="Palkkad">Palakkad</option>
                    <option value="Malappuram">Malappuam</option>
                    <option value="Kozhikod">Kozhikod</option>
                  </select>
              </div>
              <label for="">Flat, House no., Building, Company, Apartment</label>
              <div class="col-md-12 form-group p_star">
                <input type="text" class="form-control" value="<%= Address.add1 %>" id="add1" name="add1" />
                
              </div>

              <label for="">Area, Street, Sector, Village</label>
              <div class="col-md-12 form-group p_star">
                <input type="text" class="form-control" value="<%= Address.add2 %>" id="add2" name="add2" />
                
              </div>
              <label for="">Town /City</label>
              <div class="col-md-12 form-group p_star">
                <input type="text" class="form-control" value="<%= Address.Town %>" id="city" name="city" />
                
              </div>

