    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const zoomImages = document.querySelectorAll('.zoom-image');
            const zoom = mediumZoom(zoomImages, {
                background: '#000',
            });
    
            document.addEventListener('keydown', function (e) {
                if (e.key === 'Escape' && zoom.isZoomed()) {
                    zoom.close();
                }
            });
        });
    </script>

    <script>
        const zoomDiv = document.querySelector('.zoomDiv')
        const hideProductDt = document.querySelector('.hideProductDt')
        const productImage = document.querySelector('.productImage')
        const zoomedImg = document.querySelector('.zoomDiv>img')

        productImage.addEventListener('mousemove',(e) => {
            hideProductDt.style.display = 'none'
            zoomDiv.style.display = 'block'
            
            // let y = (e.clientY - 139)/(489-139)
            // let x = (e.clientX -251)/(602-251)
            // // let x = e.clientX/751
            // console.log(e.clientY,e.clientX)
            // console.log("x-y",x,y);
            // zoomedImg.style.transform = `translate3d(-${1928*2.5*x}px, -${1080*2.5*y}px, 0px)`

        const rect = productImage.getBoundingClientRect();
        const offsetX = e.clientX - rect.left;
        const offsetY = e.clientY - rect.top;

        const xRatio = offsetX / rect.width;
        const yRatio = offsetY / rect.height;

        const zoomedImageWidth = zoomedImg.naturalWidth;
        const zoomedImageHeight = zoomedImg.naturalHeight;

        const zoomDivWidth = zoomDiv.offsetWidth;
        const zoomDivHeight = zoomDiv.offsetHeight;

        const translateX = (zoomedImageWidth - zoomDivWidth) * -xRatio;
        const translateY = (zoomedImageHeight - zoomDivHeight) * -yRatio;

        zoomedImg.style.transform = `translate(${translateX}px, ${translateY}px)`;

        
        })
        productImage.addEventListener('mouseleave',(e) => {
            hideProductDt.style.display = 'block'
            zoomDiv.style.display = 'none'
        })
    </script>


