<script>
    const swal_Alert = document.getElementById("sweet_Assistant");

    if (swal_Alert) {
        swal("You have reached the maximum number of login attempts. Please reset your password.", {
            icon:"warning",
            buttons: {
                cancel: {
                    text: "Cancel",
                    value: null,
                    visible: true,
                    className: "",
                    closeModal: true,
                },
                resetPassword: {
                    text: "Forgout Password ?",
                    value: "Reset Password",
                    visible: true,
                    className: "swal-button--reset-password",
                    closeModal: false,
                },
            },
            customClass: {
                confirmButton: 'swal-button--reset-password',
                cancelButton: 'swal-button--cancel',
            },
            buttonsStyling: false,
        })

        .then((value) => {
            switch (value) {
                case "Reset Password":
                    // Your logic for resetting password
                    window.location.href = 'forgoutPassword';
                    break;
                default:
                    // Handle other cases or no action
                    break;
            }
        });
    }
</script>


