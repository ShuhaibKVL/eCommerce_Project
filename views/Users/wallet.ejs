            </div>
            
            <div class="containerr">
                <h1 class="wallet-heading " style="font-size: 2rem;">My Ecommerce Wallet</h1>
                <% if(wallet) {%>
                <div class="container-fluid" style="display: flex; flex-wrap: wrap; ">
                    <div class="wallet-details">
                      <h2 style="padding-right: 90px;"><strong>$</strong> <%= wallet.balance %></h2>
                      <p>Wallet Balance</p>
                      <!-- You can add more details here like transaction history, account number, etc. -->
                    </div>

                    <div class="wallet-details" style="margin-left: 10px;">
                        <h2>Bank Details</h2>
                        <p><strong>FDRL</strong> 001178</p>
                        <!-- You can add more details here like transaction history, account number, etc. -->
                      </div>
                </div>
                
                <h3>Transaction History</h3>
                <table class="history-table">
                  <thead style="background-color: #ffebe0;">
                    <tr>
                      <th>Date</th>
                      <th>Description</th>
                      <th>Transaction</th>
                      <th>Amount</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% wallet.history.forEach(Item => { %>
                        <tr>
                            <td><%= Item.date.toString() %></td>
                            <td><%= Item.description %></td>
                            <% if (Item.description !== "Purchased With wallet") { %>
                                <td style="color: green;" ><%= Item.transaction %></td>
                                <td style="color: green;" >+<%= Item.amount %></td>
                            <% }else{ %>
                                <td style="color: red;" ><%= Item.transaction %></td>
                                <td style="color: red;" >-<%= Item.amount %></td>
                            <% } %>
                            
                        </tr>
                    <% }) %>
                    <!-- <tr>
                      <td>2024-02-17</td>
                      <td>Received payment from Client A</td>
                      <td>$200.00</td>
                    </tr>
                    <tr>
                      <td>2024-02-16</td>
                      <td>Purchased Item B</td>
                      <td>-$50.00</td>
                    </tr> -->
                    <!-- Add more transaction rows as needed -->
                  </tbody>
                </table>
              </div>
            <% }else{ %>
                <p>Create a Wallet account</p>
                <h3 style="color: #ff3368; font-family:Georgia, 'Times New Roman', Times, serif;">Purchase with 10% discount on every purchase</h3>
            <% } %>

        </section>
    
