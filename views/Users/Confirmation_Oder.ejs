    <!--================ confirmation part start =================-->
    <section class="confirmation_part padding_top">
        <% if(typeof OrderDetails !== 'undefined' ) { %>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="confirmation_tittle">
                            <span style="color: green !important;">Thank you. Your order has been received.</span>
                        </div>
                    </div>
                    <div class="col-lg-6 col-lx-4">
                        <div class="single_confirmation_details">

                            <h4>order info</h4>
                            <ul>
                                <li>
                                    <p>order id</p><span>: <%= OrderDetails.OderId %></span>
                                </li>
                                <li>
                                    <p>date</p><span>: <%= OrderDetails.createdAt.toLocaleDateString() %></span>
                                </li>
                                <li>
                                    <p>total</p><span>: $<%= OrderDetails.totalAmount %>.00</span>
                                </li>
                                <li>
                                    <p>Payment Method</p><span>: <%= OrderDetails.PaymentMethod  %></span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6 col-lx-4">
                        <div class="single_confirmation_details">
                            <h4>Billing Address</h4>
                            <ul>
                                <li>
                                    <p>Name </p><span>: <%= OrderDetails.UserId.name %></span>
                                </li>
                                <li>
                                    <p>Phone Number</p><span>: <%= OrderDetails.UserId.phoneNumber %></span>
                                </li>
                                <li>
                                    <p>Payment Id </p><span>: <%= OrderDetails.PaymentId  %></span>
                                </li>
                                <li>
                                    <p>Email</p><span>: <%= OrderDetails.UserId.email  %></span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6 col-lx-4">
                        <div class="single_confirmation_details">
                            <h4>shipping Address</h4>
                            
                            <ul>
                                <li>
                                    <p>Street</p><span>: <%= OrderDetails.Deliver_Address.add1 %> </span>
                                </li>
                                <li>
                                    <p>city</p><span>: <%= OrderDetails.Deliver_Address.add2 %></span>
                                </li>
                                <li>
                                    <p>country</p><span>: <%= OrderDetails.Deliver_Address.Country %></span>
                                </li>
                                <li>
                                    <p>postcode</p><span>:  <%= OrderDetails.Deliver_Address.PinCode %></span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="order_details_iner">
                            <h3>Order Details</h3>
                            <table class="table table-borderless">
                                <thead>
                                    <tr>
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Total</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% OrderDetails.OrderItems.forEach(item => { %>
                                        <tr>
                                            <th colspan="2"><span><%= item.ProductId.Name %></span></th>
                                            <th>x <%= item.quantity %></th>
                                            <th> <span>$<%= item.Price %>.00</span></th>
                                        </tr>
                                    <% }) %>
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th scope="col" colspan="3">Total</th>
                                        <th scope="col">$<%= OrderDetails.totalAmount %>.00</th>
                                    </tr>
                                </tfoot>
                            </table>
                            
                        </div>
                    </div>
                </div>
            </div>
        <% } %>
        <div class="container" style="width: 100%; display: flex; justify-content: end; align-items: center; flex-wrap: wrap; margin-top: 1vw;">
            <a href="/download-invoice?oderId=<%= OrderDetails.OderId %>"><button style="border: none; color: blue; " id="downloadInvoiceBtn">Download Invoice  <i class="fa-solid fa-cloud-arrow-down fa-beat"></i></button></a>
            <a class="btn_1 checkout_btn_1" style=" background-color: rgb(255, 47, 134);" href="/LoadOrderManagment">See Orders</a>
        </div>

    </section>
