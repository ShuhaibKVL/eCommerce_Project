                    <!--================Order Area =================-->
                    <div id="reloadDiv">
                        <section class="cart_area padding_top">
                            <div class="container">
                                <div class="cart_inner">
                                    <div class="table-responsive">
                                        <% if ( Order ) { %>
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th scope="col" style="color: red;">Order Date</th>
                                                        <th scope="col" style="color: rgb(85, 196, 0);">Order ID</th>
                                                        <th scope="col">Amount</th>
                                                        <th scope="col">Orders Products</th>
                                                        <th scope="col">Order Status</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% for (let i=0; i < Order.length; i++) { %>
                                                        <tr>
                                                            <td>
                                                                <div class="media">
                                                                    <div class="media-body">
                                                                        <p style="color: red;">
                                                                            <%= Order[i].createdAt.toLocaleDateString() %>
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <h5 style="color: rgb(85, 196, 0);">
                                                                    <%= Order[i].OderId %>
                                                                </h5>
                                                            </td>
                                                            <td>
                                                                <h5> $ <%= Order[i].totalAmount %>
                                                                </h5>
                                                            </td>
                                                            <td>
                                                                <div class="media-body">
                                                                    <% for (let j=0; j < product[i].OrderItems.length;
                                                                        j++) { %>
                                                                        <p style="font-size: 14px;"><span
                                                                                style="color: orange;">
                                                                                <%= j + 1 %> :
                                                                            </span>
                                                                            <%= product[i].OrderItems[j].ProductId.Name
                                                                                %>
                                                                        </p>
                                                                        <% } %>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="media-body"
                                                                    id="refreshAccordingToBtnAction">
                                                                    <% for (let j=0; j < product[i].OrderItems.length;
                                                                        j++) { %>
                                                                        <% if(Order[i].OrderItems[j].OrderStatus=='Placed'
                                                                            ){ %>
                                                                            <p><button
                                                                                    onclick="cancelOrder('<%= Order[i].OrderItems[j].ProductId._id %>','<%= Order[i].OderId %>','<%= Order[i].UserId %> ')"
                                                                                    class="OrdeerSataus"
                                                                                    style="display: block; font-size: 12px;">
                                                                                    Cancel
                                                                                </button></p>
                                                                            <% }else
                                                                                if(Order[i].OrderItems[j].OrderStatus=='Request: cancel Order'
                                                                                ) { %>
                                                                                <p class="cancelOrder">
                                                                                    Cancel pending...
                                                                                </p>
                                                                                <% }else
                                                                                    if(Order[i].OrderItems[j].OrderStatus=='Delivered'
                                                                                    ){ %>
                                                                                    <p><button
                                                                                            onclick="ReturnOrder('<%= Order[i].OrderItems[j].ProductId._id %>','<%= Order[i].OderId %>','<%= Order[i].UserId %> ')"
                                                                                            class="OrdeerSataus"
                                                                                            style="display: block; font-size: 12px;">
                                                                                            Return
                                                                                        </button></p>
                                                                                    <% }else
                                                                                        if(Order[i].OrderItems[j].OrderStatus=='Request: Return Order'
                                                                                        ){ %>
                                                                                        <p
                                                                                            style="color: rgb(255, 123, 0); font-size: 12px;">
                                                                                            Return pending...
                                                                                        </p>
                                                                                        <% }else
                                                                                            if(Order[i].OrderItems[j].OrderStatus=='Canceled'
                                                                                            ){ %>
                                                                                            <p style="color: red;">
                                                                                                Canceled</p>
                                                                                            <% }else
                                                                                                if(Order[i].OrderItems[j].OrderStatus=='Returned'
                                                                                                ){ %>
                                                                                                <p style="color: red;">
                                                                                                    Returned</p>
                                                                                                <% } %>
                                                                                                    <% } %>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <% console.log( Order[i]._id ) %>
                                                                    <a href="/orderDetails?id=<%= Order[i]._id %>"
                                                                        style="text-decoration: none;"><i
                                                                            class="fa-solid fa-angles-right"></i></a>
                                                            </td>
                                                        </tr>
                                                        <% } %>
                                                </tbody>
                                            </table>
                                            <% } else{%>
                                                <div class="text-center d-flex justify-content-center">
                                                    <h2 style="font-family:serif">Empty Orders...! </h2>
                                                </div>
                                                <% } %>
                                    </div>
                                </div>

                                <!-- PAGINATION -->
                                <% if(totalPages) { %>
                                    <div class="col-lg-12">
                                        <div class="pageination">
                                            <nav aria-label="Page navigation example">
                                                <ul class="pagination justify-content-center">
                                                    <li class="page-item">
