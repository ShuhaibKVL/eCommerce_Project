    <script>
        function cancelOrder(ProductId, OderId, UserId) {
            console.log(ProductId);

            fetch(`http://localhost:5000/cancelOrder?id=${ProductId}&OderId=${OderId}&UserId=${UserId}`, {
                method: 'PATCH',
                headers: {
                    'Content-Type': 'application/json',
                },

            })
                .then(response => response.json())
                .then(success => {
                    if (success) {
                        $('#reloadDiv').load('/LoadOrderManagment #reloadDiv')
                    }

                })


                .catch(error => console.log("Error :", error));
        }
    </script>



    <script>
        function ReturnOrder(ProductId, OderId, UserId) {
            console.log(ProductId);
            fetch(`http://localhost:5000/ReturnOrder?id=${ProductId}&OderId=${OderId}&UserId=${UserId}`, {
                method: 'PATCH',
                headers: {
                    'Content-Type': 'application/json',
                },
            })
                .then(response => response.json())
                .then(success => {
                    // if(success){
                    //     $('#OderDetails').load('/LoadOrderManagment' ,'#OderDetails')
                    // }
                    Swal.fire({
                        icon: 'info',
                        title: 'Return Ordered',
                        text: 'The return request successfully sended ',
                        timer: 3000, // Show for 3 seconds
                        showConfirmButton: false,
                    });
                    //  Redirect to confirmation page after 3 seconds
                    setTimeout(() => {
                        $('#reloadDiv').load('/LoadOrderManagment #reloadDiv')
                    }, 3000);

                })
                .catch(error => console.log("Error :", error));
        }
    </script>

