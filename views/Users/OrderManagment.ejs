                                                    </li>
                                                    <% for(let i=1; i <=totalPages ; i++ ){ %>
                                                        <li class="page-item"><a href="/LoadOrderManagment?page=<%= i %>" class="page-link">
                                                                <%= i %>
                                                            </a></li>
                                                        <% } %>
                                                            <li class="page-item">
                                                                <a class="page-link" href="#" aria-label="Next">
                                                                    <i class="ti-angle-double-right"></i>
                                                                </a>
                                                            </li>
                                                </ul>
                                            </nav>
                                        </div>
                                    </div>
                                <% } %>
                        </section>
                    </div>
                    <!--================End Cart Area =================-->
                </div>
            </div>
    </section>
