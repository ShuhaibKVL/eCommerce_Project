                  <div class="card-body">
                    <ul class="list-inline">
                      <li id="day" class="list-inline-item"><i class="fa fa-circle mr-2 text-light" id="dayIcon" style="border:1px solid rgb(255, 255, 255); background-color: white; border-radius: 50%; height: 14px;"></i>Day</li>
                      <li id="week" class="list-inline-item"><i class="fa fa-circle mr-2 text-light" id="weekIcon" style=" border-radius: 50%; height: 14px;" ></i>Week</li>
                      <li id="month" class="list-inline-item"><i class="fa fa-circle mr-2 text-light" id="monthIcon" style=" border-radius: 50%; height: 14px;"></i>Month</li>
                    </ul>
                    <script>
                      document.getElementById('week').addEventListener('click', function(){

                        document.getElementById('weekIcon').style.border = "1px solid rgb(255, 255, 255)"
                        document.getElementById('weekIcon').style.backgroundColor = "white"

                        document.getElementById('dayIcon').style.border = "none"
                        document.getElementById('dayIcon').style.backgroundColor = "transparent"

                        document.getElementById('monthIcon').style.border = "none"
                        document.getElementById('monthIcon').style.backgroundColor = "transparent"
                      })

                      document.getElementById('month').addEventListener("click",function(){
                        document.getElementById('weekIcon').style.border = "none"
                        document.getElementById('weekIcon').style.backgroundColor = "transparent"

                        document.getElementById('dayIcon').style.border = "none"
                        document.getElementById('dayIcon').style.backgroundColor = "transparent"

                        document.getElementById('monthIcon').style.border = "1px solid rgb(255, 255, 255)"
                        document.getElementById('monthIcon').style.backgroundColor = "white"
                      })

                      document.getElementById('day').addEventListener("click",function(){
                        document.getElementById('dayIcon').style.border = "1px solid rgb(255, 255, 255)"
                        document.getElementById('dayIcon').style.backgroundColor = "white"

                        document.getElementById('weekIcon').style.border = "none"
                        document.getElementById('weekIcon').style.backgroundColor = "transparent"

                        document.getElementById('monthIcon').style.border = "none"
                        document.getElementById('monthIcon').style.backgroundColor = "transparent"
                      })
                    </script>

